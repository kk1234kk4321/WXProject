/**index.wxss**/
.container {
  flex:1;
  line-height:1.5;
  font-size:15px ;
  background-color:white;
  /* margin-top:10px; */
} 

.container-item {
  display: flex;
  flex-direction: column;
  margin-top: 14px;
}

.item{
  display: flex;
  flex-direction: row;
  align-items: center;
  border-bottom: 1px solid #DDDDDD ;
  /* padding-left: 20px; */
  background: #B9DFEB;
  width: 100%;
  margin: 3px auto;
  height: 40px;
}

.container-add{
  /* border:1px dashed #888888;
  position:fixed; bottom:5px; */
  border: 2px #4CBFE0 solid;
  border-radius: 4px;
  display: flex;
  flex-direction: row;
  width: 86%;
  height: 30px;
  align-items: center;
  color:#888888;
  margin: 0 auto;
  margin-top: 20px;
}

.container-logo {
  display: flex;
  flex-direction: column;
  padding-top: 20px;
}

.zylogo {
  width: 191px;
  height: 121px;
  margin: 0 auto;
}

.container-others {
  display: flex;
  flex-direction: row;
  margin-top: 20px;
}

.container-others image {
  width: 70px;
  height: 70px;
}

.others {
  width: 33.3%;
  float: left;
  text-align: center;
  background: white;
}

.container-bottom {
   margin-top:40px;
   height:30px;
   text-align:center;
   color:#72706B;
   font-size:14px;
}